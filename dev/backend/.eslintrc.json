{
  "root": true,
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:unicorn/recommended",
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "unicorn"
  ],
  "rules": {
    "quotes": [
      "error",
      "double",
      {
        "allowTemplateLiterals": true
      }
    ],
    "unicorn/prefer-module": "off"
  }
}
