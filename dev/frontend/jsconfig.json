{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "baseUrl": ".",
    "paths": {
      "@m/*": [
        "src/modules/*"
      ],
      "@s/*": [
        "src/styles/*"
      ],
      "@v/*": [
        "src/views/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "src/*",
    "src/**/*.vue",
    "src/*.vue"
  ],
  "exclude": [
    "node_modules",
    "public"
  ]
}
