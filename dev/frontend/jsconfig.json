{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@m/*": [
        "./src/modules/*"
      ],
      "@v/*": [
        "./src/views/*"
      ],
      "@s/*": [
        "./src/styles/*"
      ]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "public"
  ]
}
